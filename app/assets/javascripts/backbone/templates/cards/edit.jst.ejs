<h1>Edit card</h1>

<form id="edit-card" name="card">
  <div class="field">
    <label for="info"> info:</label>
    <input type="text" name="info" id="info" value="<%= info %>" >
  </div>

  <div class="field">
    <label for="name"> name:</label>
    <input type="text" name="name" id="name" value="<%= name %>" >
  </div>

  <div class="field">
    <label for="set"> set:</label>
    <input type="text" name="set" id="set" value="<%= set %>" >
  </div>

  <div class="field">
    <label for="source"> source:</label>
    <input type="text" name="source" id="source" value="<%= source %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Update Card" />
  </div>

</form>

