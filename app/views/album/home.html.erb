<script type="text/javascript">
  $(function() {
    // BancoChile is the app name
    window.app = new BancoChile.Routers.AppRouter({
        APP_ID: <%= APP_ID -%>,
        APP_SECRET: '<%= APP_SECRET -%>',
        user: <%= @user.to_json.html_safe -%>,
        ranking: <%= @ranking.to_json.html_safe -%>,
        cards: <%= @cards.to_json.html_safe -%>,
    });
    Backbone.history.start();
  });
</script>
